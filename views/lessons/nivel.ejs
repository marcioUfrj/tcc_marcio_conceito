<div class="container">
  <div id="question-container">
    <div id="question">Lista de Can-do <%= nivel_name %></div>
    <div id="answer-buttons" class="btn-grid">
      <% canDos.forEach(canDo => { %>
        <a href="/lessons/<%= nivel_name %>/<%= canDo.id %>" class="btn btn-primary">
          <%= canDo.lesson %> | <%= canDo.name %>
        </a>
      <% }) %>
    </div>
  </div>
</div>